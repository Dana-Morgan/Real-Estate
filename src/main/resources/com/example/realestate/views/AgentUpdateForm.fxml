   <Label layoutX="126.0" layoutY="120.0" text="Email" />
   <Label layoutX="126.0" layoutY="172.0" text="Phone" />
   <Label layoutX="127.0" layoutY="280.0" text="Password" />
   <Label layoutX="130.0" layoutY="14.0" text="Update Agents">
      <font>
         <Font name="System Italic" size="17.0" />
      </font>
   </Label>
    <ChoiceBox fx:id="roleChoiceBox" layoutX="122.0" layoutY="244.0">
        <items>
            <FXCollections fx:factory="observableArrayList">
